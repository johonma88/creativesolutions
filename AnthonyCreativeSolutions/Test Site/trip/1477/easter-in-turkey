 <script type="text/javascript">
	$(document).ready(function(){
		var PageName = 'index';
		if(PageName != 'commentary'){
			$('.panel-default').click(function(){
				$('.faq_rightimgactive').each(function(){
					$(this).removeClass('faq_rightimgactive');
					$(this).addClass('faq_rightimg');
				});
				$(this).children(':first').children(':first').children(':first').children(':last').removeClass('faq_rightimg');
				$(this).children(':first').children(':first').children(':first').children(':last').addClass('faq_rightimgactive');
			});
		}
		
		//$('.tripit_slider_pencil').click(function(){
			//$(this).find(".tripit_slider_pencilhover").toggle();
		//});
		
		$('#popover1').click(function(){
			$('#popoverdiv1').toggle(200);
		});
	});	
</script>

