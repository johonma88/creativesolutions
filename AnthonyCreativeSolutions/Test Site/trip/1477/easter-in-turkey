		$('#login').fancybox({
			padding:0,
			width:538,
			height:316,
			fitToView 		:	true,
			autoSize 		:	true,
			'type'				:	'iframe',
			'onComplete'	:	function(){
				$('#fancybox-content').css('cssText', 'height:'+ default_login_popup_height+'px; min-height: 0 !important; width:'+default_login_popup_width+'px');
				$('#fancybox-wrap').css('width',default_login_popup_width_wrape+'px');
//				$('#fancybox-content').css('width',default_login_popup_width+'px');
			},
			'onClosed'      : function () {
				$('#fancybox-content').css('min-height','');
			}
		});
		
		$('#signup').fancybox({
			padding:0,
			width:420,
			height:512,
			fitToView : true,
			autoSize : true,
			'type'				: 'iframe',
			'onComplete':function(){
