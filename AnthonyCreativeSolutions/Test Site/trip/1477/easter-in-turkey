            'overlayShow'	: false,
            'transitionIn'	: 'elastic',
            'transitionOut'	: 'elastic'
        });

        $('.inntab_close_cross').click(function(){
            $(this).parent().parent().remove();
        });

        $('.header_reference_iframe').click(function(){
            $('#notify').click();
        });
    });
</script>        
